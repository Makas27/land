{
  "manifest_version": 3,
  "name": "SearchAdvisor",
  "description": "New tab extension with search",
  "version": "1.2",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "./public/index.html"
  },
  "icons": {
    "16": "./assets/img/icon16.png",
    "48": "./assets/img/icon48.png",
    "128": "./assets/img/icon128.png"
  }
}